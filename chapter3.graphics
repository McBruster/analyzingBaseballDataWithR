install.packages(tidyverse, Lahman, ggplot2,dplyr)
library(tidyverse)
library(Lahman)
ls("package:Lahman")
head(AllstarFull)


hof<- HallOfFame
People("debut")
colnames()

merge(x = hof, y = People[ , c("playerID","debut", "finalGame")], by = "playerID", all.x=TRUE)


hof <- hof%>%
  mutate(MidCareer = (debut + finalGame)/ 2,
	Era = cut(MidCareer, 
		breaks = c(1800, 1900, 1919, 1941, 1960 , 1976, 1993, 2050),
	labels = c("19th Century","Dead Ball","Lively Ball","Integration",
			"Expansion","Free Agency","Long Ball")))








